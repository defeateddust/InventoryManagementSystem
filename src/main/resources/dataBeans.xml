<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	
	<context:component-scan base-package="com.revature.beans"/>
	
	<context:annotation-config/>
	
	<bean id="address" class="com.revature.beans.Address"></bean>
	<bean id="category" class="com.revature.beans.Category"></bean>
	<bean id="client" class="com.revature.beans.Client"></bean>
	<bean id="clientType" class="com.revature.beans.ClientType"></bean>
	<bean id="poLine" class="com.revature.beans.POLine"></bean>
	<bean id="product" class="com.revature.beans.Product"></bean>
	<bean id="purchaseOrder" class="com.revature.beans.PurchaseOrder"></bean>
	<bean id="state" class="com.revature.beans.State"></bean>
	
</beans>
